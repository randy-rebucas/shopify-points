<h1>{{title}}</h1>

<form action="/points">
    <input type="hidden" name="metafieldId" value="{{metafieldObj.id}}"/>
    <div>
        <label for="wedgit-title">Wedgit Title</label>
        <input type="text" name="title" value="{{metavalue.pointWidgetTitle}}"/>
    </div>
    <div>
        <label for="wedgit-title">Is Rounded Corner</label>
        <select name="isRounded">
            <option>--</option>
            <option value="true" {{#select metavalue.isRounded}}>Yes</option>
            <option value="false" {{#select metavalue.isRounded}}>No</option>
        </select>
    </div>
    <div>
        <input type="submit" name="submit" value="Submit"/>
    </div>
</form>
